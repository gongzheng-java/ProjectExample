<script type="text/javascript">
function doUpload() {  
    var formData = new FormData($( "#myform" )[0]);  
    var file=$("aa").val();
    alert(file);
    $.ajax({  
         url: 'upload/fileupload' ,  
         type: 'POST',  
         data: formData,  
         async: false,  
         cache: false,  
         contentType: false,  
         processData: false,  
         success: function (returndata) {  
             //alert(returndata);  
         },  
         error: function (returndata) {  
             //alert(returndata);  
         }  
    });  
}
</script>
